
<RelativeLayout android:id="@+id/relativeLayout1"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<ImageButton android:layout_width="wrap_content" android:layout_centerHorizontal="true"
		android:id="@+id/beginDragger" android:background="@drawable/drag_button_background" android:layout_height="20px"></ImageButton>
	<FrameLayout android:id="@+id/frameLayout1" android:layout_width="fill_parent"
		android:background="#518d02" android:layout_above="@+id/endDragger" android:layout_height="fill_parent" android:layout_below="@+id/beginDragger">
	</FrameLayout>
	<ImageButton android:layout_centerHorizontal="true"
		android:layout_width="wrap_content" android:id="@+id/endDragger" android:background="@drawable/drag_button_background" android:layout_above="@+id/okButton" android:layout_height="20px"></ImageButton>
	<ImageButton android:layout_width="wrap_content"
		android:layout_toRightOf="@+id/endDragger" android:id="@+id/okButton" android:background="@drawable/ok_button_background" android:layout_alignParentBottom="true" android:layout_height="20px"></ImageButton>
	<ImageButton android:layout_width="wrap_content" android:layout_toLeftOf="@+id/endDragger"
		android:id="@+id/cancelButton" android:background="@drawable/cancel_button_background" android:layout_alignParentBottom="true" android:layout_height="20px"></ImageButton>
</RelativeLayout>
